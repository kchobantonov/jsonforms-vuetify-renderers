{
  "name": "@jsonforms/vue-vuetify",
  "version": "3.2.0-preview-alpha.3",
  "description": "Vue Vuetify renderers for JSON Forms",
  "repository": "https://github.com/eclipsesource/jsonforms-vuetify-renderers",
  "bugs": "https://github.com/eclipsesource/jsonforms/issues",
  "homepage": "http://jsonforms.io/",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vue",
    "vuetify",
    "form",
    "forms",
    "json",
    "jsonforms",
    "frontend",
    "generator",
    "input",
    "renderengine",
    "jsonschema",
    "schema",
    "uischema",
    "layout",
    "customization"
  ],
  "main": "lib/jsonforms-vue-vuetify.cjs.js",
  "module": "lib/jsonforms-vue-vuetify.esm.js",
  "jsdelivr": "lib/jsonforms-vue-vuetify.umd.js",
  "unpkg": "lib/jsonforms-vue-vuetify.umd.js",
  "styles": "lib/jsonforms-vue-vuetify.css",
  "typings": "lib/index.d.ts",
  "type": "module",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "run-p type-check \"build-only {@}\" --",
    "watch": "vite build --watch",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "clean": "rimraf lib coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path ../.gitignore",
    "format": "prettier --write src/",
    "test": "vitest --watch=false",
    "test:coverage": "vitest --coverage --watch=false"
  },
  "peerDependencies": {
    "@jsonforms/core": "3.2.1",
    "@jsonforms/vue": "3.2.1",
    "@mdi/font": "^7.4.47",
    "ajv": "^8.6.1",
    "dayjs": "^1.10.6",
    "lodash": "^4.17.15",
    "maska": "^2.1.11",
    "vue": "^3.4.21",
    "vuetify": "^3.6.6"
  },
  "devDependencies": {
    "@jsonforms/core": "3.2.1",
    "@jsonforms/vue": "3.2.1",
    "@mdi/font": "^7.4.47",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.14.172",
    "@types/node": "^20.12.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.6.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "ajv": "^8.6.1",
    "core-js": "^3.9.1",
    "cross-env": "^7.0.2",
    "dayjs": "^1.10.6",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "eslint-plugin-vue-scoped-css": "^2.8.0",
    "jsdom": "^24.0.0",
    "lodash": "^4.17.15",
    "maska": "^2.1.11",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^4.4.1",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-node-polyfills": "^0.21.0",
    "vitest": "^1.4.0",
    "vue": "^3.4.21",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^2.0.11",
    "vuetify": "^3.6.6"
  }
}
